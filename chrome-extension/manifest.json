{
  "manifest_version": 3,
  "name": "AI Autofill Form Filler",
  "version": "1.0",
  "description": "Autofill job forms with AI-extracted data from your uploaded PDFs.",
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "host_permissions": [
    "http://localhost:8000/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  }
} 